    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>0cb37262-7690-4e37-8905-b315a8e1b7a9</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>..\..</DockerfileContext>
